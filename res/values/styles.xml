    <style name="Theme.Actionbarcustomgo" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_actionbarcustomgo</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Actionbarcustomgo</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Actionbarcustomgo</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Actionbarcustomgo</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Actionbarcustomgo</item>
        <item name="android:actionBarStyle">@style/ActionBar.Transparent.Actionbarcustomgo</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_actionbarcustomgo</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_actionbarcustomgo</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Actionbarcustomgo</item>
    </style>

    <style name="ActionBar.Solid.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_actionbarcustomgo</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_actionbarcustomgo</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_actionbarcustomgo</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Actionbarcustomgo</item>
    </style>

    <style name="ActionBar.Transparent.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_actionbarcustomgo</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Actionbarcustomgo</item>
    </style>

    <style name="PopupMenu.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_actionbarcustomgo</item>	
    </style>

    <style name="DropDownListView.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_actionbarcustomgo</item>
    </style>

    <style name="ActionBarTabStyle.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_actionbarcustomgo</item>
    </style>

    <style name="DropDownNav.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_actionbarcustomgo</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_actionbarcustomgo</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_actionbarcustomgo</item>
    </style>
    
    <style name="ProgressBar.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_actionbarcustomgo</item>
    </style>
    
    <style name="ActionButton.CloseMode.Actionbarcustomgo" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_actionbarcustomgo</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Actionbarcustomgo.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Actionbarcustomgo</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Actionbarcustomgo</item>
    </style>
