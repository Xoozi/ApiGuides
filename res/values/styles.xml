<resources>

    <color name="custom_action_bar_theme_color_background">#b0b0ff</color>

    <style name="CustomActionBarThemeOverlay"
            parent="@android:style/Theme.Holo">
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="CustomActionBarThemeBackground" 
            parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBarBackground</item>
    </style>

    <style name="MyActionBarBackground"
            parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/custom_action_bar_theme_color_background</item>
    </style>

    <!-- A theme for a custom dialog appearance.  Here we use an ugly
         custom frame. -->
    <style name="Theme.CustomDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/shape_fill_box</item>
    </style>

